{
  "name": "package.js",
  "author": "Stephan Ahlf",
  "preferGlobal": false,
  "main": "./index.js",
  "license": "MIT",
  "description": "A node.js package loader to distribute plugins via package distribution networks like npm",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/s-a/package.js.git"
  },
  "scripts": {
    "test": "mocha",
    "cover-html": "jscoverage lib lib-cov && mocha -R html-cov > coverage.html && rm lib-cov/*.*",
    "docs-test": "node ./lib/index.js --file=use-case-main.js --describe=* --output=lib-doc/use-case-main.js.MD",
    "docs": "node ./lib/index.js --file=lib/deep.js --describe=Deep --output=lib-doc/api-package.js.MD"
  },
  "engines": {
    "node": ">=0.8.0"
  },
  "dependencies": {
  },
  "devDependencies": {
    "mocha": "^2.2.4",
    "should": "^6.0.1",
    "jscoverage": "^0.5.9",
    "mocha-lcov-reporter": "0.0.2",
    "coveralls": "^2.11.2"
  }
}